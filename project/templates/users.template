<table class="table table-striped">
    <thead>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Nickname</th>
    </thead>
    <tbody>
        <% _.each(users, function(user){ %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.nickName %></td>
            </tr>
        <% }); %>
    </tbody>
</table>